<div class="padding-navbar">
		<h1>My Profile</h1>
		Profile Picture:<br>
		Name: <%= @profile.name %><br>
		Email: <%= @profile.email %><br>
		Birthdate: <%= @profile.birth %><br>
		Gender: <%= @profile.gender %><br>
		Phone number: <%= @profile.phone_number %><br>
		<%= link_to 'Edit Profile', edit_account_registration_path %>

		<h2>Tournaments I play in</h2>
		<% if (@profile.tournaments_played.empty? == false) then %>
				<% @profile.tournaments_played.each do |key, value| %>
						<p><%= link_to value, tournaments_show_path(:tournamet_id => key)%></p>
				<% end %>
		<% else %>
				<p>You have't joined any tournament yet</p>
		<% end %>

		<h2>Tournaments I organize</h2>
		<% if (@profile.tournaments_orgnized.empty? == false) then %>
				<% @profile.tournaments_orgnized.each do |key, value| %>
						<p><%= link_to value, tournaments_show_path(:tournament_id => key)%></p>
				<% end %>
				<%= link_to "arrange group", organizers_show_path %>
		<% else %>
				<p>You haven't orgnized any tournamnet yet</p>
		<% end %>
		<h2> Tournament I am a Sub-Admin</h2>
		<% if (@profile.tournaments_sub_admin.empty? == false) then %>
				<% @profile.tournaments_sub_admin.each do |key, value| %>
						<p><%= link_to value, tournaments_show_path(:tournament_id => key)%></p>
				<% end %>
				<%= link_to "arrange group", sub_admins_show_path %>
		<% else %>
				<p>You haven't orgnized any tournamnet yet</p>
		<% end %>

		<h2>Sponsor Info</h2>
				<p>Sponsored Tournament</p>
				<table>
					<thead>
							<th>Sponsor Name</th>
							<th>Sponsored Tournament</th>
					</thead>
					<tbody>

			        <% Sponsor.where(account_id: current_account.id).each do |t| %>
								<% Sponsorship.where(sponsor_id: t.id).each do |s|%>
				        <tr>
				            <td><%= link_to Sponsor.find(t.id).name, sponsors_show_path(:sponsor_id => t.id)%></td>
				            <td><%= link_to Tournament.find(s.tournament_id).name, tournaments_show_path(:tournament_id => s.tournament_id)%></td>
				        </tr>
			        	<% end %>
							<% end %>
			    </tbody>
				</table>
		<h2>Payment Info</h2>
				<p>Credit card info: *******0000</p>
</div>
